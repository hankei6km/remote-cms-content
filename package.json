{
  "name": "remote-cms-contents",
  "version": "0.1.1-pre.20",
  "description": "Mirror contents of remote cms to local files for nuxt-content",
  "author": "hankei6km <hankei6km@gmail.com> (https://github.com/hankei6km)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/hankei6km/remote-cms-contents.git"
  },
  "bugs": {
    "url": "https://github.com/hankei6km/remote-cms-contents/issues"
  },
  "keywords": [
    "cms",
    "nuxt",
    "nuxt-content"
  ],
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "rcc": "dist/main.js"
  },
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "npm run clean && tsc && rimraf dist/test && mv dist/src/* dist/ && rimraf dist/src",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "clean": "rimraf \"dist/*\"",
    "upgrade-interactive": "npm-check --update",
    "csb:test": "npm test -- --runInBand --watchAll",
    "mapconfig-schema:build": "sh scripts/mapconfig-schema-build.sh"
  },
  "dependencies": {
    "@hankei6km/rehype-image-salt": "^0.1.1-pre.27",
    "ajv": "^8.6.3",
    "axios": "^0.21.4",
    "gray-matter": "^4.0.3",
    "image-size": "^1.0.0",
    "jsonata": "^1.8.5",
    "rehype": "^12.0.0",
    "rehype-parse": "^8.0.3",
    "rehype-remark": "^9.0.0",
    "rehype-sanitize": "^5.0.0",
    "rehype-split-paragraph": "^0.2.2",
    "rehype-stringify": "^9.0.2",
    "remark-stringify": "^10.0.1",
    "unified": "^10.1.0",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@types/yargs": "^17.0.3",
    "jest": "^27.2.5",
    "jest-mock-axios": "^4.4.1",
    "nodemon": "^2.0.13",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4"
  }
}
